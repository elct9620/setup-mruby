{
  "name": "setup-mruby-action",
  "version": "0.1.0",
  "private": true,
  "description": "The action to setup mruby",
  "main": "lib/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "rollup -c rollup.config.mjs",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elct9620/setup-mruby.git"
  },
  "keywords": [
    "actions",
    "mruby",
    "setup"
  ],
  "author": "Aotokitsuruya",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^3.0.0",
    "@actions/exec": "^3.0.0",
    "@actions/io": "^3.0.2"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/node": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^10.0.0",
    "eslint-plugin-jest": "^29.12.2",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "js-yaml": "^4.1.1",
    "prettier": "^3.8.1",
    "rollup": "^4.57.1",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
